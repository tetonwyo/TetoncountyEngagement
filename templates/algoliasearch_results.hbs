            </a>
            <h4>{{formatDate PublishDate}}</h4>
            <p>{{chop Description 200}}</p>
        </div>
        {{/each_upto}}
      </div>
    </script>
    <script id="docResultsTemplateMore" type="text/x-handlebars-template">
      <ul id="docMoreResults" class="results doc">
        <h2>More Doc Results</h2>
        {{#each_upto docs 4 100}}
        <li class="doc-result-row">
            <a href="{{url}}">
                <i class="fa fa-file-text fa-4x"></i>
                <div class="title">{{title}}</div>
                <!-- <h3>{{formatDate PublishDate}}</h3> -->
                <div class="subtitle">{{meetingTitle}}</div>
            </a>
        </li>
        {{/each_upto}}
    </ul>
    </script>
