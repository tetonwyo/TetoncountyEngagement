	{{#each_upto docs 4}}
		<div class="doc-result-row">
		    <a href={{url}}>
		        <h2>{{title}}</h2>
		        <h3>{{PublishDate}}</h3>
		        <h3>{{meetingTitle}}</h3>
		    </a>
		</div>
	{{/each_upto}}
</div>
