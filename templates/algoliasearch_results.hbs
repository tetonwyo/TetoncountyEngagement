 <script id="webResultsTemplate" type="text/x-handlebars-template">
      <div id="webResults" class="">
        <!-- <h2>Web Results</h2> -->
        {{#each_upto web 0 3}}
            <div class="web-result-row">
                <a href="{{Url}}">
                    <h2>{{Title}} <span class='tiny'><i class="fa fa-bars"></i> {{Type}}</span></h2>
                </a>
                <h4>{{formatDate PublishDate}}</h4>
                <p>{{chop Description 200}}</p>
            </div>
        {{/each_upto}}
      </div>
    </script>
    <script id="docResultsTemplate" type="text/x-handlebars-template">
      <ul id="docResults" class="">
        <!-- <h2>Doc Results</h2> -->
        {{#each_upto docs 0 3}}
            <li class="doc-result-row">
                <a href="{{url}}">
                    <i class="fa fa-file-text fa-4x"></i>
                    <div class="title">{{chop title 25}}</div>
                    <!-- <h3>{{formatDate PublishDate}}</h3> -->
                    <div class="subtitle">{{meetingTitle}}</div>
                </a>
            </li>
        {{/each_upto}}
      </ul>
    </script>

    <!-- Definitely a more efficient way to do these more templates.. -->

    <script id="webResultsTemplateMore" type="text/x-handlebars-template">
      <div id="webMoreResults" class="results web">
